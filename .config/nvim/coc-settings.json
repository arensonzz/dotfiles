{
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": "f"
  },
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 3,
  "coc.source.floaterm.enable": false,

  "diagnostic.displayByAle": false,

  "coc.preferences.hoverTarget": "float",
  "coc.preferences.colorSupport": true,

  "jedi.enable": true,
  "jedi.startupMessage": true,
  "jedi.markupKindPreferred": "plaintext",
  "jedi.jediSettings.autoImportModules": ["numpy", "matplotlib"],
  "jedi.executable.command": "jedi-language-server",
  "jedi.executable.args": [],
  "jedi.completion.disableSnippets": false,
  "jedi.diagnostics.enable": false,
  "jedi.diagnostics.didOpen": false,
  "jedi.diagnostics.didChange": false,
  "jedi.diagnostics.didSave": false,
  "jedi.workspace.extraPaths": [],

  "html.enable": true,

  "eslint.enable": false,
  "tsserver.enable": true,

  "tsserver.npm": "/usr/bin/npm",

  "suggest.noselect": false,
  "snippets.extends": {
      "python": ["django"]
  },

  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "gosnippets/UltiSnips"
  ]
}
