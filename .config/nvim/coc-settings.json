{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "languageserver": {},
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": "f",
    "enum": "E",
    "struct": "S",
    "snippet": "S",
    "class": "C",
    "constructor": "Ct",
    "interface": "I",
    "module": "M",
    "typeParameter": "T",
    "field": "m_",
    "property": "m_",
    "enumMember": "m_E",
    "variable": "v",
    "constant": "k",
    "unit": "U",
    "color": "clr",
    "value": "val",
    "keyword": "key",
    "file": "file",
    "reference": "ref",
    "folder": "fldr",
    "event": "evnt",
    "operator": "op",
    "default": ""
  },
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 3,
  "suggest.noselect": false,
  "diagnostic.displayByAle": true,
  "diagnostic.enable": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.signPriority": 9999999,
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.ultisnips.pythonPrompt": false,
  "snippets.loadFromExtensions": true,
  "inlayHint.enable": true,
  "inlayHint.enableParameter": false,
  "html.format.enable": false,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "html.jinja",
    "jinja.html",
    "htmljinja"
  ],
  "html.validate.styles": false,
  "html.validate.scripts": false,
  "html.autoClosingTags": false,
  "htmldjango.formatting.provider": "djlint",
  "htmldjango.djlint.addFormatterArgs": [
    "--indent 2",
    "--max-blank-lines 1",
    "--max-line-length 120",
    "--profile django",
  ],
  "htmldjango.djlint.formatJs": true,
  "htmldjango.djlint.formatCss": true,
  "eslint.enable": true,
  "tsserver.enable": true,
  "cmake.lsp.enable": true,
  "rust-analyzer.diagnostics.enable": false,
  "sql.database": "postgresql",
  "clangd.arguments": [
    "--header-insertion=never"
  ],
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/19.1.2/clangd_19.1.2/bin/clangd",
  "clangd.compilationDatabaseCandidates": [
    "${workspaceFolder}/debug",
    "${workspaceFolder}/Debug",
    "${workspaceFolder}/build-aarch64",
    "${workspaceFolder}/build",
    "${workspaceFolder}/Build"
  ],
  "bitbake.pathToBitbakeFolder": "/space/yocto/layers/poky/bitbake",
  "bitbake.pathToBuildFolder": "/space/yocto/builds/lsp-build",
  "bitbake.pathToEnvScript": "/space/yocto/layers/poky/oe-init-build-env",
  "bitbake.parseOnSave": false,
}
