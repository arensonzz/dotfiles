{
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": "f"
  },
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 3,
  "suggest.noselect": false,

  "diagnostic.displayByAle": true,
  "diagnostic.enable": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.signPriority": 9999999,


  "jedi.enable": true,
  "jedi.startupMessage": true,
  "jedi.markupKindPreferred": "plaintext",
  "jedi.jediSettings.autoImportModules": ["numpy", "matplotlib"],
  "jedi.executable.command": "/home/arensonz/.local/bin/jedi-language-server",
  "jedi.executable.args": [],
  "jedi.completion.disableSnippets": false,
  "jedi.diagnostics.enable": false,
  "jedi.diagnostics.didOpen": false,
  "jedi.diagnostics.didChange": false,
  "jedi.diagnostics.didSave": false,
  "jedi.workspace.extraPaths": [],

  "html.enable": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "html.jinja",
    "jinja.html",
    "htmljinja"
  ],

  "eslint.enable": true,
  "tsserver.enable": true,
  "tsserver.npm": "/usr/bin/npm",

  "snippets.extends": {
      "python": ["django"]
  },
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.loadFromExtensions": true,

  "coc.preferences.extensionUpdateCheck": "weekly",

  "cmake.lsp.enable": true,
  "clangd.disableDiagnostics": true,

  "svelte.enable-ts-plugin": true,
  "svelte.plugin.svelte.diagnostics.enable": true,
  "svelte.plugin.css.diagnostics.enable": false,
  "svelte.plugin.typescript.diagnostics.enable": true,
  "html.autoClosingTags": false,
  "svelte.plugin.html.tagComplete.enable": false,

  "rust-analyzer.diagnostics.enable": false,
  "rust-analyzer.diagnostics.enableExperimental": false,

  "sql.database": "postgresql"
}
